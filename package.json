{
  "name": "ReactStockAnalyzerDemo",
  "version": "1.0.0",
  "private": true,
  "description": "ReactStockAnalyzerDemo demo application",
  "author": "Serg volk",
  "homepage": "https://magsoft2.github.io/ReactStockAnalyzerDemo",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/development.js",
    "build": "cross-env NODE_ENV=production webpack --display-modules --config ./webpack/production.js",
    "build_dev": "cross-env NODE_ENV=development webpack --display-modules --config ./webpack/development_build.js",
    "build_prod": "cross-env NODE_ENV=production webpack --display-modules --config ./webpack/production.js",
    "build_for_deploy": "cross-env NODE_ENV=production ENV_PUBLIC_PATH='/ReactStockAnalyzerDemo' webpack --display-modules --config ./webpack/production_deploy.js",
    "deploy_git": "gh-pages -d build",
    "test": "cross-env NODE_ENV=test SUPPRESS_NO_CONFIG_WARNING=true jest"
  },
  "jest": {
    "verbose": true,
	"testEnvironment": "<rootDir>/__mocks__/setupEnvironment.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
	"globals": {
      "__DEV__": true
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePaths": [
      "src", "config"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
